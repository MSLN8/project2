<div class="login">
<h2>Log in</h2>

{{> flash_messages}}

<form action="/process-login" method="POST">
  <label>
    Email :
    <input type="email" name="email" placeholder = "Enter your email here"/>
  </label>
  <br>

  <label>
    Password :
    <input type="password" name="originalPassword" placeholder = "Enter your password here"/>
  </label>
  <br>

  <button>Log In</button>
</form>
 <button><a href="/google/login"> Login with Google</a></button>
<br/> 
</div>