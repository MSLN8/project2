<h3>User List</h3>

<ul>
  {{#each userArray}}
  <li>
    <h4>{{this.fullName}}</h4>
    <ul>
      <li>Email: {{this.email}}</li>
      <li>Registered: {{this.createdAt}}</li>
      <li>Zodiac Sign: {{this.zodiac}}</li>
      <li>Location: {{this.location}}</li>
      <li>{{this.avatar}}</li>
    </ul>
  </li>
  {{/each}}
</ul>

